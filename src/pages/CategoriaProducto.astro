---
import Layout from "../layouts/Layout.astro";
import {NavbarPrincipal} from "../sections/navbars/navbar.jsx";
import {ProductoHero} from "../sections/CategoriaProducto/ProductoFiltro.jsx";
import Footer from "../sections/footer.astro";
import {TipoProducto} from "../sections/CategoriaProducto/TipoProducto.jsx";

---


<Layout title="FreshFusion">
  <header  class="relative">
    <NavbarPrincipal posicion={"relative"} bg={"bg-white"}  client:idle />
  </header>
  <main class="animate-blurred-fade-in">
    <ProductoHero client:idle />
    <TipoProducto client:idle />
  </main>
  <footer>
    <Footer />
  </footer>
</Layout>

