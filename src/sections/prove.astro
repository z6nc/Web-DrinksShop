---
import { LogoProductos } from "./proveedores.ts";
---

<section class="hidden  bg-white lg:flex w-auto h-28 items-center justify-center gap-7 mx-6  lg:my-8 ">
  {
    LogoProductos.map(({ id, logo, url }) => (
      <a
        target="_blank"
        class="w-[7%]  cursor-pointer  "
        rel="noopener noreferrer"
        href={url}
      >
        <img class="object-cover" src={logo} alt={id} />
      </a>
    ))
  }
</section>




