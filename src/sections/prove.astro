---
import { LogoProductos } from "./proveedores.ts";
---

<section
  class="hidden lg:flex w-auto h-28 items-center justify-center gap-7 mx-6"
>
  {
    LogoProductos.map(({ id, logo, url }) => (
      <a
        target="_blank"
        class="w-[6%] hover:scale-105 transition cursor-pointer"
        rel="noopener noreferrer"
        href={url}
      >
        {" "}
        <img class="object-cover " src={logo} alt={id} />
      </a>
    ))
  }
</section>


